{
  "name": "@ourofoundation/ouro-js",
  "version": "2.1.3",
  "description": "Interact with the Ouro platform in browser and node.js environments",
  "module": "./dist/index.esm.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "tsc --emitDeclarationOnly --declaration",
    "prepublishOnly": "npm run build"
  },
  "author": "ourofoundation",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ourofoundation/ouro-js.git",
    "directory": "src"
  },
  "devDependencies": {
    "@swc/core": "^1.11.4",
    "tslib": "^2.3.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "marked": "^16.0.0",
    "uuidv7": "^1.0.0",
    "zod": "^4.0.5"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
