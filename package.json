{
  "name": "@ourofoundation/ouro-js",
  "version": "3.0.0",
  "description": "Interact with the Ouro platform in browser and node.js environments",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./schema/*": {
      "types": "./dist/schema/*.d.ts",
      "import": "./dist/schema/*.js",
      "require": "./dist/schema/*.cjs",
      "default": "./dist/schema/*.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs",
      "default": "./dist/utils/*.js"
    },
    "./utils/air/*": {
      "types": "./dist/utils/air/*.d.ts",
      "import": "./dist/utils/air/*.js",
      "require": "./dist/utils/air/*.cjs",
      "default": "./dist/utils/air/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "tsc --emitDeclarationOnly --declaration",
    "prepublishOnly": "npm run build"
  },
  "author": "ourofoundation",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ourofoundation/ouro-js.git",
    "directory": "src"
  },
  "devDependencies": {
    "@swc/core": "^1.11.4",
    "tslib": "^2.3.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "marked": "^16.0.0",
    "uuidv7": "^1.0.0",
    "zod": "^4.0.5"
  },
  "typesVersions": {
    "*": {
      "schema/*": [
        "dist/schema/*"
      ],
      "utils/*": [
        "dist/utils/*"
      ],
      "utils/air/*": [
        "dist/utils/air/*"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
